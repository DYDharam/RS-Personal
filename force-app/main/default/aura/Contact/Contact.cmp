<aura:component description="Contact" controller="MyProfilePageController">

    <aura:attribute name="mapMarkers" type="Object"/>
    <aura:attribute name="zoomLevel" type="Integer" />
    <aura:attribute name="contactUsInfo" type="sObject" default="{'sobjectType': 'Contact_Us__c'}" />
    <aura:attribute name="currentUserProfileDetails" type="sObject" default="{'sobjectType': 'User'}" />
    <aura:handler name="init" value="{! this }" action="{! c.init }"/>


    <div class="demo-only demo-only--sizing slds-grid slds-wrap">
        <div class="slds-size_1-of-12">
            &nbsp;
        </div>
        <div class="slds-size_10-of-12">
            <div class="slds-box_x-small slds-text-align_center  slds-m-bottom_large cusHeadingCol">
